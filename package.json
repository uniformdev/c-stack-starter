{
  "name": "@uniformdev/c-stack-starter",
  "version": "2.0.239",
  "private": true,
  "engines": {
    "npm": ">=8.19.0",
    "node": ">=18.12.0",
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm"
  },
  "scripts": {
    "dev": "run-s uniform:pull:manifest dev:next",
    "dev:next": "next dev --port 3000",
    "build": "run-s uniform:pull:manifest build:next",
    "build:next": "next build",
    "start": "next start",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "uniform:pull:manifest": "uniform context manifest download --output ./src/context/manifest.json",
    "uniform:pull:datatype": "uniform canvas datatype pull ./content/datatypes",
    "uniform:pull:components": "uniform canvas component pull ./content/components",
    "uniform:pull:compositions": "uniform canvas composition pull ./content/compositions",
    "uniform:pull:project-map-definitions": "uniform project-map definition pull ./content/maps",
    "uniform:pull:project-map-nodes": "uniform project-map node pull ./content/nodes",
    "uniform:pull:context:signals": "uniform context signal pull ./content/signals",
    "uniform:pull:context:enrichments": "uniform context enrichment pull ./content/enrichments",
    "uniform:pull": "run-s uniform:pull:datatype uniform:pull:components uniform:pull:compositions uniform:pull:project-map-definitions uniform:pull:project-map-nodes",
    "uniform:push:datatype": "uniform canvas datatype push ./content/datatypes",
    "uniform:push:components": "uniform canvas component push ./content/components",
    "uniform:push:compositions": "uniform canvas composition push ./content/compositions",
    "uniform:push:project-map-definitions": "uniform project-map definition push ./content/maps",
    "uniform:push:project-map-nodes": "uniform project-map node push ./content/nodes",
    "uniform:push:context:signals": "uniform context signal push ./content/signals",
    "uniform:push:context:enrichments": "uniform context enrichment push ./content/enrichments",
    "uniform:push": "run-s uniform:push:datatype uniform:push:components uniform:push:compositions uniform:push:project-map-definitions uniform:push:project-map-nodes",
    "uniform:composition:publish": "uniform canvas composition push ./content --state published",
    "uniform:manifest:publish": "uniform context manifest publish",
    "uniform:publish": "run-s uniform:composition:publish uniform:manifest:publish",
    "import:products": "ts-node src/scripts/import-products.ts"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^16.0.3",
    "@contentful/rich-text-types": "^16.0.3",
    "@uniformdev/canvas": "19.15.0",
    "@uniformdev/canvas-next": "19.15.0",
    "@uniformdev/canvas-react": "19.15.0",
    "@uniformdev/context": "19.15.0",
    "@uniformdev/context-devtools": "19.15.0",
    "@uniformdev/context-gtag": "19.15.0",
    "@uniformdev/context-next": "19.15.0",
    "@uniformdev/context-react": "19.15.0",
    "@uniformdev/project-map": "19.15.0",
    "classnames": "^2.3.2",
    "lodash.debounce": "^4.0.8",
    "next": "13.4.0",
    "nextjs-cors": "^2.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-multi-carousel": "^2.8.3",
    "sass": "^1.62.1",
    "sharp": "^0.32.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.32",
    "@types/react-dom": "^18.0.11",
    "@uniformdev/canvas-enhancers": "^18.9.1-alpha.11",
    "@uniformdev/cli": "19.15.0",
    "autoprefixer": "^10.4.14",
    "contentful": "^9.3.5",
    "eslint-config-custom": "*",
    "npm-run-all": "^4.1.5",
    "shared": "*",
    "tailwind-config": "*",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4"
  }
}
